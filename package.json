{
  "name": "sovd-simulation",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "generate:types": "openapi-typescript docs/api-specs/sovd-api.yaml -o src/generated/sovd.d.ts",
