    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "jose": "^5.2.2",
    "@prisma/client": "^5.8.0"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "^20.11.19",
    "openapi-typescript": "^6.7.3",
    "prisma": "^5.8.0"
  }
}
