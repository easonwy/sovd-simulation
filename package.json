{
  "name": "sovd-simulation",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "generate:types": "openapi-typescript docs/api-specs/sovd-api.yaml -o src/generated/sovd.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "node scripts/api-smoke.mjs",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev --name",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@tanstack/react-table": "^8.21.3",
    "jose": "^5.2.2",
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.19",
    "autoprefixer": "^10.4.22",
    "jest": "^29.7.0",
    "openapi-typescript": "^6.7.3",
    "postcss": "^8.5.6",
    "prisma": "^5.8.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}